.cu-list.menu {
  height: 64rpx;
}

.cu-list.menu>.cu-item {
  background-color: transparent;
}
.flex{
  align-items: center;
}
.item {
  display: inline-block;
  width: 22px;
  /* margin-right: 5px; */
  color: #fff;
  /* font-size: 12px; */
  text-align: center;
  background-color: #1989fa;
  border-radius: 2px;
}

.wrap {
  width: 100%;
  height: 376rpx;
  position: absolute;
  top: -36rpx;
  left: 36rpx;
  overflow: hidden;
}

.wrap:before {
  content: "";
  display: block;
  border-radius: 16px 16px 0px 0px;
  width: 80rpx;
  height: 16rpx;
  position: absolute;
  right: 200rpx;
  /* background: #4D6530; */
}

.wrap:after {
  content: "";
  display: block;
  border-radius: 0px 16rpx 16rpx 0px;
  width: 16rpx;
  height: 80rpx;
  position: absolute;
  right: 0px;
  top: 200rpx;
  /* background: #4D6530; */
}

.ribbon6 {
  display: inline-block;
  text-align: center;
  width: 400rpx;
  height: 64rpx;
  line-height: 64rpx;
  position: absolute;
  top: 60rpx;
  right: -100rpx;
  z-index: 2;
  overflow: hidden;
  transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  opacity: 0.8;
  backdrop-filter:blur(3px);
  backdrop-filter: brightness(160%);
  /* border: 1px dashed; */
  /* color: #39b54a; */
  /* box-shadow: 0 0 0 3px #57DD43, 0px 21px 5px -18px rgba(0, 0, 0, 0.6); */
  /* background: #39b54a42; */
}
